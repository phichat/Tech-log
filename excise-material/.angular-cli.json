{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "excise-material"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../src/assets/plugins/bootstrap/css/bootstrap.min.css",
        "../src/assets/css/style.css",
        "../src/assets/css/colors/blue.css",
        "../src/assets/plugins/bootstrap-datepicker/bootstrap-datepicker.min.css",
        "../src/assets/plugins/bootstrap-daterangepicker/daterangepicker.css",
        "../src/assets/plugins/typeahead.js-master/dist/typehead-min.css",
        "styles.scss"
      ],
      "scripts": [
        "../src/assets/plugins/jquery/jquery.min.js",
        "../src/assets/plugins/bootstrap/js/popper.min.js",
        "../src/assets/plugins/bootstrap/js/bootstrap.min.js",
        "../src/assets/plugins/bootstrap-datepicker/bootstrap-datepicker.min.js",
        "../src/assets/plugins/bootstrap-datepicker/locales/bootstrap-datepicker.th.min.js",
        "../src/assets/plugins/bootstrap-daterangepicker/daterangepicker.js",
        "../src/assets/js/jquery.slimscroll.js",
        "../src/assets/js/sidebarmenu.js",
        "../src/assets/plugins/sparkline/jquery.sparkline.min.js",
        "../src/assets/js/custom.js",
        "../src/assets/js/pace.min.js",
        "../src/assets/plugins/datatables/jquery.dataTables.min.js",
        "../src/assets/plugins/datatables/dataTables.bootstrap.js",
        "../src/assets/plugins/styleswitcher/jQuery.style.switcher.js",
        "../src/assets/plugins/typeahead.js-master/dist/typeahead.bundle.min.js",
        "../src/assets/plugins/typeahead.js-master/dist/bloodhound.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}